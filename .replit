[deployment]
deploymentTarget = "cloudrun"
run = [
  "gunicorn",
  "--bind",
  "0.0.0.0:5000",
  "--workers",
  "2",
  "--timeout",
  "120",
  "backend.app:create_app()",
]
build = ["npm", "run", "build"]
